        // Fetch the current visit count from the API and update the page
        function updateVisitCount() {
            fetch('/api/visits')
                .then(response => response.json())
                .then(data => {
                    document.getElementById('counter').innerText = data.visitCount;
                })
                .catch(error => console.error('Error:', error));
        }
        // ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏î‡∏∂‡∏á‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏ú‡∏π‡πâ‡πÉ‡∏ä‡πâ‡πÅ‡∏•‡∏∞‡πÅ‡∏™‡∏î‡∏á‡∏ú‡∏•‡∏ö‡∏ô‡∏´‡∏ô‡πâ‡∏≤‡πÄ‡∏ß‡πá‡∏ö
    function checkLoginStatus() {
        fetch('/getUser')
            .then(response => response.json())
            .then(data => {
                if (data.loggedin) {
                    // ‡πÅ‡∏™‡∏î‡∏á‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÉ‡∏ä‡πâ‡∏ñ‡πâ‡∏≤‡∏•‡πá‡∏≠‡∏Å‡∏≠‡∏¥‡∏ô‡πÅ‡∏•‡πâ‡∏ß
                    document.getElementById('usernameDisplay').innerText = "Welcome, " + data.username + "!";
                    
                    // ‡πÅ‡∏™‡∏î‡∏á‡∏õ‡∏∏‡πà‡∏° Log Out
                    document.getElementById('logoutButton').style.display = "block";

                    // ‡∏ã‡πà‡∏≠‡∏ô‡∏õ‡∏∏‡πà‡∏° Log In
                    document.getElementById('loginButton').style.display = "none";

                } else {
                    // ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡∏•‡πá‡∏≠‡∏Å‡∏≠‡∏¥‡∏ô ‡πÅ‡∏™‡∏î‡∏á‡πÄ‡∏õ‡πá‡∏ô Guest
                    document.getElementById('usernameDisplay').innerText = "Welcome, Guest!";
                    
                    // ‡∏ã‡πà‡∏≠‡∏ô‡∏õ‡∏∏‡πà‡∏° Log Out
                    document.getElementById('logoutButton').style.display = "none";

                    // ‡πÅ‡∏™‡∏î‡∏á‡∏õ‡∏∏‡πà‡∏° Log In
                    document.getElementById('loginButton').style.display = "block";
